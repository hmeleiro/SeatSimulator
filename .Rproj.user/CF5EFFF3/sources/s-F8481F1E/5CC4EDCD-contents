p <- 4

t <- y[y$Código.de.Provincia == p,]

parties <- unique(t$siglas) # Crea un objeto con los partidos en la provincia p
votos <- unique(t$sobrevalido) # Crea un objeto con los votos a los partidos de la provincia p
escaños <- unique(t$seats) # Crea un objeto con el número de escaños a repartir en la provincia p
prov <- unique(t$Nombre.de.Provincia) # Crea un objeto con el nombre de la provincia p

d <- seats_ha(parties = parties, 
              votes = votos, 
              n_seats = escaños,
              method = "dhondt")
